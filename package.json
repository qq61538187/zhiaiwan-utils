{
  "name": "zhiaiwan-utils",
  "version": "1.0.0",
  "description": "智AI湾前端工具集",
  "keywords": "智AI湾, zhiaiwan, utils, 工具库, 工具包",
  "sideEffects": false,
  "engineStrict": true,
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "node build/build.js",
    "build:es": "node build/build.js mode=es",
    "lint": "npx prettier --write ./src",
    "changeset:version": "changeset version",
    "changeset:publish": "changeset publish",
    "check": "biome check --write --verbose"
  },
  "license": "MIT",
  "devDependencies": {
    "@vitejs/plugin-vue-jsx": "^3.0.1",
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "child_process": "^1.0.2",
    "less": "^4.1.2",
    "prettier": "3.0.3",
    "qunit-extras": "^3.0.0",
    "qunitjs": "^2.4.1"
  },
  "dependencies": {
    "@biomejs/biome":  "1.9.4",
    "@changesets/cli": "^2.29.5",
    "chalk": "4",
    "closure-compiler": "0.2.12",
    "download-git-repo": "^3.0.2",
    "glob": "7.1.1",
    "handlebars": "^4.7.7",
    "log-symbols": "4",
    "ora": "5",
    "semver": "5.3.0",
    "uglify-js": "2.7.5"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "@algolia/client-search"
      ]
    }
  }
}
